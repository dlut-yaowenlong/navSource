{
  "welcomeFile": "/apps/sourcepage/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/apps/(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-runtime",
      "destination": "html5-apps-repo-runtime",
      "authenticationType": "none"
    },
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/odata/v4/$1",
      "destination": "srv-api",
      "csrfProtection": true
    }
  ]
}
